{
  "name": "kiddohood",
  "version": "1.0.0",
  "description": "KiddoHood is a platform where parents can find and share activities, locate parks and playgrounds, and discover local events suitable for children. This platform will integrate various APIs to provide comprehensive information and features to enhance the user experience.",
  "main": "index.js",
  "scripts": {
    "install": "npm install --prefix client && npm install --prefix server",
    "start": "concurrently \"npm run start:server\" \"npm run dev:client\"",
    "start:server": "npm run start --prefix server",
    "dev:client": "npm run dev --prefix client",
    "build": "npm run build --prefix client",
    "lint:client": "npm run lint --prefix client",
    "seed:server": "npm run seed --prefix server",
    "test:server": "npm run test --prefix server",
    "render-build": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Design-For-Reap/KiddoHood.git"
  },
  "author": "Alex Da Silva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alex-Design-For-Reap/KiddoHood/issues"
  },
  "homepage": "https://github.com/Alex-Design-For-Reap/KiddoHood#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1"
  }
}
